  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonstar-x/automation-service/issues"
  },
  "homepage": "https://github.com/moonstar-x/automation-service#readme",
  "dependencies": {
    "@octokit/webhooks-types": "^7.1.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "humanize-duration": "^3.29.0",
    "level": "^8.0.0",
    "module-error": "^1.0.2",
