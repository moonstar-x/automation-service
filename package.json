    "@types/node": "^20.4.2",
    "@types/on-finished": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-greencoast": "^0.0.4",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
